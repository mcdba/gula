#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/ssf/0.10.3/node_modules/ssf/bin/ssf.njs/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/ssf/0.10.3/node_modules/ssf/bin/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/ssf/0.10.3/node_modules/ssf/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/ssf/0.10.3/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/ssf/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules:/Users/kmb/js/gula/node_modules:/Users/kmb/js/node_modules:/Users/kmb/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ssf/0.10.3/node_modules/ssf/bin/ssf.njs" "$@"
else 
  exec node  "$basedir/../../../../../../ssf/0.10.3/node_modules/ssf/bin/ssf.njs" "$@"
fi
