#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/adler-32/1.2.0/node_modules/adler-32/bin/adler32.njs/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/adler-32/1.2.0/node_modules/adler-32/bin/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/adler-32/1.2.0/node_modules/adler-32/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/adler-32/1.2.0/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/adler-32/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules:/Users/kmb/js/gula/node_modules:/Users/kmb/js/node_modules:/Users/kmb/node_modules:/Users/node_modules:/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules/adler-32/bin/adler32.njs/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules/adler-32/bin/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules/adler-32/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../adler-32/bin/adler32.njs" "$@"
else 
  exec node  "$basedir/../adler-32/bin/adler32.njs" "$@"
fi
