#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/printj/1.1.2/node_modules/printj/bin/printj.njs/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/printj/1.1.2/node_modules/printj/bin/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/printj/1.1.2/node_modules/printj/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/printj/1.1.2/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/printj/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules:/Users/kmb/js/gula/node_modules:/Users/kmb/js/node_modules:/Users/kmb/node_modules:/Users/node_modules:/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules/printj/bin/printj.njs/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules/printj/bin/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules/printj/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../printj/bin/printj.njs" "$@"
else 
  exec node  "$basedir/../printj/bin/printj.njs" "$@"
fi
