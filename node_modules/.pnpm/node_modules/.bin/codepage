#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/codepage/1.14.0/node_modules/codepage/bin/codepage.njs/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/codepage/1.14.0/node_modules/codepage/bin/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/codepage/1.14.0/node_modules/codepage/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/codepage/1.14.0/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/codepage/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules:/Users/kmb/js/gula/node_modules:/Users/kmb/js/node_modules:/Users/kmb/node_modules:/Users/node_modules:/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules/codepage/bin/codepage.njs/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules/codepage/bin/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules/codepage/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../codepage/bin/codepage.njs" "$@"
else 
  exec node  "$basedir/../codepage/bin/codepage.njs" "$@"
fi
