#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/xlsx/0.15.6/node_modules/xlsx/bin/xlsx.njs/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/xlsx/0.15.6/node_modules/xlsx/bin/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/xlsx/0.15.6/node_modules/xlsx/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/xlsx/0.15.6/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/xlsx/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/kmb/js/gula/node_modules/.pnpm/node_modules:/Users/kmb/js/gula/node_modules:/Users/kmb/js/node_modules:/Users/kmb/node_modules:/Users/node_modules:/node_modules:/Users/kmb/js/gula/node_modules/xlsx/bin/xlsx.njs/node_modules:/Users/kmb/js/gula/node_modules/xlsx/bin/node_modules:/Users/kmb/js/gula/node_modules/xlsx/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../xlsx/bin/xlsx.njs" "$@"
else 
  exec node  "$basedir/../xlsx/bin/xlsx.njs" "$@"
fi
